{
    "routes": [
      {
        "route": "/profile",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/profile/*",
        "serve": "/index.html",
        "statusCode": 200
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*", "/api/*"]
    }
  }